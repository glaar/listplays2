<% @youtubelist.each do |youtubelist| %>
  <div class="youtubeitem"> 

    <iframe width="320" height="195" src="//www.youtube.com/embed/<%= youtubelist.url.html_safe %>" frameborder="0" allowfullscreen>f</iframe>
    <br/>
    <% if logged_in? && current_user == @user %>
      <%#= link_to "Edit", edit_youtubelist_path(youtubelist) %>
      <%= link_to "Delete", youtubelist_delete_path(youtubelist), class: 'btn btn-default btn-xs' %>

    <% end %>
  </div>
<% end %>
